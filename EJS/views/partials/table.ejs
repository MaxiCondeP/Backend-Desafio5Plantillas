<% if (products.length!=0) { %>
  <table class="table table-dark table-hover">
  <thead>
    <tr>
      <th scope="col">ID</th>
      <th scope="col">Nombre</th>
      <th scope="col">Precio</th>
      <th scope="col">Imagen</th>
    </tr>
  </thead>
  <tbody>
    <% products.forEach(function(prod) {%>
      <tr>
       <th scope="row"><%= prod.id %></th>
        <td><%= prod.title %></td>
        <td><%= prod.price %></td>
        <td > <img class= "icon" src= <%= prod.thumbnail %> <alt=""></td>
      </tr>
      <% }); %>
  </tbody>

</table>
<% }else{ %>
  <h2>No hay productos</h2>
  <% } %>

<a href="http://localhost:8080/"><button type="button" class="btn btn-primary">Volver al formulario</button></a>
